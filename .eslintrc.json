{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes" : [
            "error",
            "double"
        ],
        "semi" : [
            "error",
            "always"
        ]
    }
}
